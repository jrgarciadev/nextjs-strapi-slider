{
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ],
      [
        "module-resolver",
        {
          "root": ["./"],
          "alias": {
            "@components": "./src/components",
            "@graphql": "./src/graphql",
            "@hooks": "./src/hooks",
            "@lib": "./src/lib",
            "@styles": "./src/styles",
            "@themes": "./src/themes",
            "@utils": "./src/utils",
            "@layouts": "./src/layouts"
          }
        }
      ]
    ]
  }